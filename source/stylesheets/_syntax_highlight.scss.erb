<%= Rouge::Themes::Github.render(:scope => '.highlight') %>

.highlight {
  background: none;
  font-size: 0.9em;
	tab-size: 2;

	.c1, .cm, .c, .cd, .cp {
	  color: #665;
	}
	.m, .mi {
		color: #007575;
	}
	.na, .nb {
		color: #007197;
	}
	.s, .s1, .s2 {
		color: #cf0f3f;
	}
  .err {
    background-color: transparent;
  }
}